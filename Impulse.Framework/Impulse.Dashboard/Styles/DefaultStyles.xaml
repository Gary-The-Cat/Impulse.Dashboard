<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Impulse.Dashboard.Styles">

    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor1" Color="RED" />

    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor2" Color="RED" />

    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor3" Color="RED" />

    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor4" Color="RED" />

    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor5" Color="RED" />

    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor6" Color="RED" />

    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor7" Color="RED" />
    
    <Style TargetType="{x:Type Button}">
        <Setter Property="Background" Value="{DynamicResource BackgroundHighlightBrush}" />
        <Setter Property="Foreground" Value="{DynamicResource ForegroundBrush}" />
    </Style>

    <Style TargetType="{x:Type ListBoxItem}">
        <Style.Resources>
            <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}"
                             Color="Transparent"/>
        </Style.Resources>
    </Style>
</ResourceDictionary>